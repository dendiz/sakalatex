<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="CommectActionSet"
            visible="true"
            id="com.swe573.sakalatex.actionSet">
         <action
               class="com.swe573.sakalatex.actions.CommentInAction"
               icon="icons/left.ico"
               id="com.swe573.sakalatex.actions.commentin"
               label="comment-in"
               style="push"
               toolbarPath="sakaGroup"
               tooltip="comment-in">
         </action>
         <action
               class="com.swe573.sakalatex.actions.CommentOutAction"
               icon="icons/right.ico"
               id="com.swe573.sakalatex.actions.commentout"
               label="comment-out"
               toolbarPath="sakaGroup"
               tooltip="comment-out">
         </action>
         <action
               class="com.swe573.sakalatex.actions.FootnoteAction"
               id="com.swe573.sakalatex.actions.footnote"
               label="footnote"
               style="push"
               toolbarPath="sakaGroup"
               tooltip="footnote">
         </action>
         <action
               class="com.swe573.sakalatex.actions.HyperlinkAction"
               icon="icons/hyperlink.ico"
               id="com.swe573.sakalatex.actions.hyperlink"
               label="hyperlink"
               style="push"
               toolbarPath="sakaGroup"
               tooltip="hyperlink">
         </action>
      </actionSet>
   </extension>
   <extension
         id="com.swe573.sakalatex.PDFBuilder"
         name="com.swe573.sakalatex.PDFBuilder"
         point="org.eclipse.core.resources.builders">
      <builder>
        <run
               class="com.swe573.sakalatex.PDFBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="com.swe573.sakalatex.SakalatexProjectNatureId"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.swe573.sakalatex.SakalatexProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            class="com.swe573.sakalatex.SakalatexWizard"
            id="com.swe573.sakalatex.wizard1"
            name="SakalatexProjectWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.swe573.sakalatex.SakalEditor"
            contributorClass="com.swe573.sakalatex.SakalatexActionBarContributor"
            default="true"
            extensions="tex"
            icon="icons/defter.ico"
            id="com.swe573.sakalatex.sakalatexEditor"
            name="SakalatexEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.swe573.sakalatex.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.swe573.sakalatex.SakalatexPreferencePage"
            id="com.swe573.sakalatex.SakalatexPreferencePage"
            name="Sakalatex Pref">
      </page>
   </extension>

</plugin>
