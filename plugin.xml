<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="com.swe573.sakalatex.actionSet">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Sample Action"
               icon="icons/sample.gif"
               class="com.swe573.sakalatex.actions.SampleAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="com.swe573.sakalatex.actions.SampleAction">
         </action>
      </actionSet>
   </extension>
   <extension
         id="com.swe573.sakalatex.PDFBuilder"
         name="com.swe573.sakalatex.PDFBuilder"
         point="org.eclipse.core.resources.builders">
      <builder>
        <run
               class="com.swe573.sakalatex.PDFBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="com.swe573.sakalatex.SakalatexProjectNatureId"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.swe573.sakalatex.SakalatexProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            class="com.swe573.sakalatex.SakalatexWizard"
            id="com.swe573.sakalatex.wizard1"
            name="SakalatexProjectWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.swe573.sakalatex.SakalEditor"
            contributorClass="com.swe573.sakalatex.SakalatexActionBarContributor"
            default="true"
            extensions="tex"
            id="com.swe573.sakalatex.sakalatexEditor"
            name="SakalatexEditor">
      </editor>
   </extension>

</plugin>
